    "# find Factorial Recursively\n",
    "\n",
    "def findFactorialRecursively(fact):\n",
    "#     print(fact)\n",
    "    if fact == 1:\n",
    "        return fact\n",
    "    else :\n",
    "        return fact * findFactorialRecursively(fact-1)\n",
    "        \n",
    "    \n",
    "findFactorialRecursively(4)"
   ]
  }
